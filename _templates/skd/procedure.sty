\def\custompagenumberformat{%
}

\def\customresetpagenumber{%
  \setcounter{page}{1}
}

% When the appendices start, add the Chapter Name (CNV) back in so it says
% "Appendix A" instead of "A.0", make it centered, reset the page numbering,
% change page number format
\newcommand\beginappendices {
    % Set up the appendix mode and modify the LaTex toc behavior
    \makeatletter
    \ChNameVar {\raggedright\normalsize \py@HeaderFamily}
    \ChNumVar  {\raggedright\Large      \py@HeaderFamily}
    \ChTitleVar{\raggedright\Large      \py@HeaderFamily}

    \renewcommand{\DOCH}{%
        %\Large\CNV\FmN{\@chapapp}\space \CNoV\thechapter
        \py@TitleColor\CNV\FmN{\@chapapp}\space\CNoV\thechapter\space\space
    }
    \renewcommand{\thepage}{\Alph{chapter}-\arabic{page}}
    \titleformat{\chapter}{\customresetpagenumber\Large\py@HeaderFamily\centering}%
            {\py@TitleColor APPENDIX \thechapter}{.5em}{\py@TitleColor}{\py@NormalColor}
    \renewcommand{\thetable}{\Alph{chapter}-\arabic{table}}
    \renewcommand{\thefigure}{\Alph{chapter}-\arabic{figure}}
    \makeatother
    \def \appendixname {\Large\py@HeaderFamily\py@TitleColor Appendix}
    \appendix
} 

